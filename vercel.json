{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "github": {
    "enabled": true,
    "comments": true,          // 启用 PR 评论
    "autoJobCancelation": true,
    "previewProduction": false,
    "branchFilter": [
      "main",
      "dev",
      "feature/*"
    ]
  },
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist"
}